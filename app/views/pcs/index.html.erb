<p id="notice"><%= notice %></p>

<h1>Pcs</h1>

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Number</th>
      <th>Status</th>
      <th>Programas</th><!--
      <th>Row</th>
      <th>Position in row</th> -->
      <th>Reclamações</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pcs.each do |pc| %>
      <tr>
        <td><%= pc.number %></td>
        <td><%= pc.status %></td>
        <td><%= pc.programs %></td> <!--
        <td><%# pc.row %></td>
        <td><%# pc.position_in_row %></td> -->
        <td><%= pc.complain %></td>
        <td><%= link_to 'Ver', pc, class: "btn btn-info" %></td>
        <td><%= link_to 'Editar', edit_pc_path(pc), class: "btn btn-warning" %></td>
        <td><%= link_to 'Remover', pc, method: :delete, class: "btn btn-danger", data: { confirm: 'Tem Certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Pc', new_pc_path %>
